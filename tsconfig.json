{
	"compilerOptions": {
		"baseUrl": "/",
		"paths": {
			"@/*": ["src/*"],
			"@components/*": ["src/components/*"],
			"@context/*": ["src/context/*"],
			"@assets/*": ["src/assets/*"],
			"@img/*": ["src/assets/images/*"],
			"@pages/*": ["src/pages/*"],
			"@utils/*": ["src/utils/*"],
			"router/*": ["src/router/*"],
			"api/*": ["src/api/*"]
		}
	},
	"include": ["src"]
}
